    </fieldUpdates>
    <rules>
        <fullName>CH_OED_AutoPopulated</fullName>
        <actions>
            <name>CH_OED_Action</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Outage End Date only filled in only for the Support Tickets with &quot;Outage&quot; : &quot;Yes&quot; and NOKIASC-27404:Restored Date is changed.</description>
        <formula>AND( 				ISCHANGED( CH_SystemRestored__c ), 				RecordType.DeveloperName = &quot;CH_NokiaIncidentWithRestoration&quot;, 				(ISPICKVAL( Severity__c , &apos;Critical&apos;)  || ISPICKVAL( Severity__c , &apos;Major&apos;)),   				ISPICKVAL( CH_Outage__c , &apos;Yes&apos;) )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
