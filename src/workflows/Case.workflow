    <fieldUpdates>
        <fullName>ERM_Related</fullName>
        <field>OwnerId</field>
        <lookupValue>Tier_2_GSS</lookupValue>
        <lookupValueType>Queue</lookupValueType>
        <name>ERM Related</name>
