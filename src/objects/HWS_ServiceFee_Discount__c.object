    <validationRules>
        <fullName>HWS_Validation_Fee_ForPercentage</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
HWS_Damaged_HW_RepairDiscount__c &gt; 1.0,
HWS_Damaged_HW_RepairDiscount__c &lt; 0.0,
HWS_Damaged_HW_RepairDiscount2__c &gt; 1.0,
HWS_Damaged_HW_RepairDiscount2__c &lt; 0.0,
HWS_Damaged_HW_ReplacementDiscount__c &gt; 1.0,
HWS_Damaged_HW_ReplacementDiscount__c &lt; 0.0,
HWS_HW_AssessmentDiscount__c &gt; 1.0,  
HWS_HW_AssessmentDiscount__c &lt; 0.0,
HWS_HW_ReplacementDiscount__c &gt; 1.0,  
HWS_HW_ReplacementDiscount__c &lt; 0.0,
HWS_Unreturned_Part_Discount__c &gt; 1.0,  
HWS_Unreturned_Part_Discount__c &lt; 0.0,
HWS_Repacking_HW_PackingDiscount__c &gt; 1.0,  
HWS_Repacking_HW_PackingDiscount__c &lt; 0.0,
HWS_Documentation_Discount__c &gt; 1.0,
HWS_Documentation_Discount__c &lt; 0.0
)</errorConditionFormula>
        <errorMessage>Discount must be between 0 and 100%.</errorMessage>
    </validationRules>
