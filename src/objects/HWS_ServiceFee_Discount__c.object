    <validationRules>
        <fullName>HWS_ServiceContractLineItem_Update</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED(HWS_Service_Contract_Line_Item__c)</errorConditionFormula>
        <errorDisplayField>HWS_Service_Contract_Line_Item__c</errorDisplayField>
        <errorMessage>Service contract line item can not be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HWS_ServiceContract_Update</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED(HWS_Service_Contract__c)</errorConditionFormula>
        <errorDisplayField>HWS_Service_Contract__c</errorDisplayField>
        <errorMessage>Service Contract can not be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HWS_ValidateAccountonServiceContract</fullName>
        <active>true</active>
        <description>HWSDDP-66</description>
        <errorConditionFormula>OR(
AND(NOT(ISBLANK(  HWS_Quotation_Contact__c)),
HWS_Quotation_Contact__r.AccountId !=   HWS_Service_Contract__r.Account.Parent.Id),

AND(NOT(ISBLANK(  HWS_Customer_Contact_2__c )),
 HWS_Customer_Contact_2__r.AccountId !=   HWS_Service_Contract__r.Account.Parent.Id))</errorConditionFormula>
        <errorMessage>Customer Contacts should belong to Customer associated with the Legal Entity of Service Contract</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HWS_ValidateInactiveContacts</fullName>
        <active>true</active>
        <description>HWSDDP-66</description>
        <errorConditionFormula>OR( AND(
NOT(ISBLANK(HWS_Quotation_Contact__c)),ISPICKVAL(  HWS_Quotation_Contact__r.Contact_Status__c,&quot;Inactive&quot;)),
AND(
NOT(ISBLANK( HWS_Customer_Contact_2__c )),ISPICKVAL( HWS_Customer_Contact_2__r.Contact_Status__c,&quot;Inactive&quot;))
)</errorConditionFormula>
        <errorMessage>The selected Customer Contact is Inactive in SRM.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HWS_ValidateNokiaEmail</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(ISBLANK( HWS_Nokia_Email__c )),
NOT(CONTAINS(LOWER($Setup.Nokia_Employee_Email_Domains__c.Nokia_Valid_Domain__c),
&apos;&quot;&apos; &amp; LOWER(TRIM(RIGHT(HWS_Nokia_Email__c, LEN(HWS_Nokia_Email__c) - FIND(&quot;@&quot;, HWS_Nokia_Email__c)))) &amp; &apos;&quot;&apos;
))
)</errorConditionFormula>
        <errorDisplayField>HWS_Nokia_Email__c</errorDisplayField>
        <errorMessage>Nokia Email field must have a valid Nokia Email Domain</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HWS_Validate_AdminDays</fullName>
        <active>false</active>
        <errorConditionFormula>OR(ISNULL(HWS_PreliminaryQuote_Expiration_Days__c),HWS_PreliminaryQuote_Expiration_Days__c &lt; 0)</errorConditionFormula>
        <errorDisplayField>HWS_PreliminaryQuote_Expiration_Days__c</errorDisplayField>
        <errorMessage>Quote days should not be blank and less than 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HWS_Validate_Discount</fullName>
        <active>true</active>
        <description>HWSDDP-8</description>
        <errorConditionFormula>OR(
AND(INCLUDES (HWS_SupportedQuoteTypes__c, &quot;Damaged Part&quot;), ISBLANK(HWS_Damaged_HW_RepairDiscount__c)),
AND(INCLUDES (HWS_SupportedQuoteTypes__c, &quot;Damaged Part&quot;), ISBLANK(HWS_Damaged_HW_ReplacementDiscount__c)),
AND(INCLUDES (HWS_SupportedQuoteTypes__c, &quot;Damaged Part&quot;), ISBLANK(HWS_Documentation_Discount__c)),
AND(INCLUDES (HWS_SupportedQuoteTypes__c, &quot;Damaged Part&quot;), ISBLANK(HWS_HW_AssessmentDiscount__c)),
AND(INCLUDES (HWS_SupportedQuoteTypes__c, &quot;Damaged Part&quot;), ISBLANK(HWS_HW_ReplacementDiscount__c)),
AND(INCLUDES (HWS_SupportedQuoteTypes__c, &quot;Damaged Part&quot;), ISBLANK(HWS_Repacking_HW_PackingDiscount__c)),
AND(INCLUDES (HWS_SupportedQuoteTypes__c, &quot;Damaged Part&quot;), ISBLANK(HWS_Damaged_HW_RepairDiscount2__c)),
AND(INCLUDES(HWS_SupportedQuoteTypes__c, &quot;Unreturned Part&quot;), ISBLANK(HWS_Unreturned_Part_Discount__c))
				)</errorConditionFormula>
        <errorMessage>If Supported Quote Types is Damaged , please fill all discount except HW Unreturned Part Discount
If Supported Quote Types is Unreturned ,please fill HW Unreturned Part Discount</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HWS_Validate_EmailTemplate</fullName>
        <active>true</active>
        <description>This is Created as a Part of HWSDDP-140.</description>
        <errorConditionFormula>ISBLANK( HWS_Email_Template__c )</errorConditionFormula>
        <errorDisplayField>HWS_Email_Template__c</errorDisplayField>
        <errorMessage>Email Template not Specified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HWS_Validate_InactiveUser</fullName>
        <active>true</active>
        <errorConditionFormula>OR(HWS_Service_Assurance_Manager__r.IsActive == False,ISBLANK(HWS_Service_Assurance_Manager__c)  )</errorConditionFormula>
        <errorDisplayField>HWS_Service_Assurance_Manager__c</errorDisplayField>
        <errorMessage>Service Assurance manager can not be inactive and blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HWS_Validate_Parts</fullName>
        <active>true</active>
        <description>HWSDDP-8</description>
        <errorConditionFormula>OR(
INCLUDES (HWS_SupportedQuoteTypes__c, &quot;Unreturned Part&quot;) &amp;&amp; isblank (HWS_Scope_of_work_Unreturned_Parts_id__c),

INCLUDES(HWS_SupportedQuoteTypes__c, &quot;Damaged Part&quot;) &amp;&amp; isblank (HWS_Scope_of_work_Damaged_Parts_id__c)
)</errorConditionFormula>
        <errorMessage>If Supported Quote Types is Damaged , please fill Scope of work Damaged Parts
If Supported Quote Types is Unreturned  ,please fill Scope of work Unreturned Parts</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HWS_Validate_QuoteDays</fullName>
        <active>true</active>
        <description>HWSDDP-8</description>
        <errorConditionFormula>OR(
				AND(INCLUDES(HWS_SupportedQuoteTypes__c, &quot;Unreturned Part&quot;),ISBLANK(HWS_Quote_Eligilibilty_Days__c)),
				AND(INCLUDES(HWS_SupportedQuoteTypes__c, &quot;Damaged Part&quot;),ISBLANK(HWS_Quote_Expiration_Days__c)))</errorConditionFormula>
        <errorMessage>If Supported Quote Types is Unreturned , please fill Quote Eligilibilty Days
If Supported Quote Types is Damaged ,please fill Quote Expiration Days</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HWS_Validate_SuspensionEmailText</fullName>
        <active>true</active>
