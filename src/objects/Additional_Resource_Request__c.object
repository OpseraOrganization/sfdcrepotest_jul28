<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>USA_Pricing_Not_Allowed</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Profile.Name != &apos;Data Loader Profile&apos;,
ISPICKVAL( Additional_Resource__c , &quot;USA Pricing&quot;),
Partner_Account__r.GEOLevel1Name__c != &apos;Region North America&apos;)</errorConditionFormula>
