<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>This custom setting is used for storing url&apos;s for edp callouts</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Batch_Size__c</fullName>
        <description>Specify the limit of records returned in the response</description>
        <externalId>false</externalId>
        <inlineHelpText>Specify the limit of records returned in the response</inlineHelpText>
        <label>Batch Size</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Custom_Query__c</fullName>
        <description>This is used when there is a failure on a specific PMDSOR prepared/callout URL and you want to run only that URL</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <inlineHelpText>Example Custom Query (to be used without the &quot;quotes&quot;): 
      SalesItems -&gt; &quot;?$expand=MainAttributes&amp;$orderby=NSNCode&amp;$skip=105600&amp;$top=100&quot;
      PCI -&gt; &quot;?$expand=MainAttributes(%24filter%3DName%20eq%20\&apos;NSN%20Finance%20Reporting%20Structure\&apos;)%2CRelationInfo(%24filter%3DRelType%20eq%20\&apos;EBOM\&apos;%20or%20RelType%20eq%20\&apos;Classified%20item\&apos;)&amp;$orderby=NSNCode&amp;$skip=600&amp;$top=10&quot;</inlineHelpText>
        <label>Custom Query</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Full_Load_Filter__c</fullName>
        <description>This field is used for giving filter criteria for full load</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <inlineHelpText>Enter filter criteria for full load like SourceTable=&quot;ALPIM&quot;. Use only characters =,!=,&lt;,&gt;. If you have multiple criteria, separate it by &apos;,&apos;.
For PMDSOR, use oData filter value</inlineHelpText>
        <label>Full Load Filter</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Full_Load__c</fullName>
        <defaultValue>false</defaultValue>
        <description>This field is used for checking whether it is a full load or delta load</description>
        <externalId>false</externalId>
        <label>Full Load</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Maximum_Timestamp__c</fullName>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <label>Maximum Timestamp</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>Product_Code__c</fullName>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <inlineHelpText>Use NSNCode for Configuration Items,ItemId for Portfolio Items, NSNCode for ProductSalesItems, ParentID for Product Relations</inlineHelpText>
        <label>Product Code</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>URL__c</fullName>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <label>URL</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Url</type>
    </fields>
    <fields>
        <fullName>isActive__c</fullName>
        <defaultValue>false</defaultValue>
        <description>This field is used for EDP product relations url&apos;s</description>
        <externalId>false</externalId>
        <label>isActive</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>PMDI_URLs</label>
    <listViews>
        <fullName>EDP_URL</fullName>
        <columns>NAME</columns>
        <columns>URL__c</columns>
        <columns>isActive__c</columns>
        <columns>Maximum_Timestamp__c</columns>
        <columns>Full_Load__c</columns>
        <columns>Full_Load_Filter__c</columns>
        <columns>Product_Code__c</columns>
        <filterScope>Everything</filterScope>
        <label>EDP URL</label>
        <language>en_US</language>
    </listViews>
    <visibility>Public</visibility>
</CustomObject>
