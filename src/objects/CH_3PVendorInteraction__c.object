    <validationRules>
        <fullName>CH_3PVendorInteractionType_StatusCreated</fullName>
        <active>true</active>
        <description>https://jiradc2.int.net.nokia.com/browse/NOKIASC-26232
3P Vendor Interaction Type Filtered based on 3P Vendor Case Status</description>
        <errorConditionFormula>AND(ISNEW() ,
   CH_Initial_Case_Status__c   =  &quot;Created&quot;,
   NOT(ISPICKVAL(CH_Type__c, &quot;Open Case&quot;))
)</errorConditionFormula>
        <errorDisplayField>CH_Type__c</errorDisplayField>
        <errorMessage>If the 3P Vendor Case status is &quot;Created&quot; then only the allowed Interaction type is: Open Case</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CH_3PVendorInteractionType_StatusPV</fullName>
        <active>true</active>
        <description>https://jiradc2.int.net.nokia.com/browse/NOKIASC-26232</description>
        <errorConditionFormula>AND(ISNEW() ,
CH_Initial_Case_Status__c = &quot;Pending Vendor&quot;,
AND(
				NOT(ISPICKVAL(CH_Type__c, &quot;On Site arrival&quot;)),
				NOT(ISPICKVAL(CH_Type__c, &quot;On Hold Period Start&quot;)),
				NOT(ISPICKVAL(CH_Type__c, &quot;Solution Received&quot;)),
				NOT(ISPICKVAL(CH_Type__c, &quot;Information Request&quot;)),
				NOT(ISPICKVAL(CH_Type__c, &quot;Update&quot;))
))</errorConditionFormula>
        <errorDisplayField>CH_Type__c</errorDisplayField>
        <errorMessage>If the 3P Vendor Case status is &quot;Pending Vendor&quot; then only the allowed Interaction types are :On Site Arrival,On Hold Period Start,Solution Received,Information Request,Update</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CH_3PVendorInteraction_ReadonlyType</fullName>
        <active>false</active>
        <description>The &quot;Type&quot; field will be read only while editing.</description>
        <errorConditionFormula>ISCHANGED(CH_Type__c)</errorConditionFormula>
        <errorDisplayField>CH_Type__c</errorDisplayField>
        <errorMessage>This field cannot be modified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CH_3PVendor_ReadonlyOriginType</fullName>
        <active>true</active>
        <description>The &quot;Origin&quot; and &quot;TYpe&quot;  field will be read only while editing.</description>
        <errorConditionFormula>OR(
				ISCHANGED(CH_Origin__c),
				ISCHANGED(CH_Type__c)
			)</errorConditionFormula>
        <errorMessage>Fields Type/Origin  cannot be modified.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CH_Occurrence_date_should_not_be_future</fullName>
        <active>true</active>
        <description>NOKIASC-24652</description>
        <errorConditionFormula>CH_OccurrenceDate__c &gt;  NOW()</errorConditionFormula>
        <errorMessage>Occurrence Date shouldn&apos;t be set as future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CH_ReminderBeInFuture</fullName>
        <active>true</active>
        <description>NOKIASC-24652</description>
        <errorConditionFormula>CH_Reminder__c &lt; Now()</errorConditionFormula>
        <errorMessage>Reminder Date should be in the Future</errorMessage>
    </validationRules>
</CustomObject>
