    <fields>
        <fullName>CLM_Generate_Contract_Summary__c</fullName>
        <externalId>false</externalId>
        <formula>IF (
AND(
OR(ISPICKVAL( Apttus__Status__c ,&quot;Fully Signed&quot;),
ISPICKVAL(Apttus__Status_Category__c,&quot;In Effect&quot;),
ISPICKVAL(Apttus__Status_Category__c,&quot;Terminated&quot;),
ISPICKVAL(Apttus__Status_Category__c,&quot;Expired&quot;),
ISPICKVAL(Apttus__Status_Category__c,&quot;Amended&quot;),
ISPICKVAL(Apttus__Status_Category__c,&quot;Renewed&quot;)
),
RecordType.DeveloperName &lt;&gt; &apos;NDA&apos;,
OR( $User.Id = OwnerId,
CONTAINS($Label.CLM_Profiles, $Profile.Name)),
$Permission.CLM_Contract_Manager_Permission),

IF(OR($User.UIThemeDisplayed == &apos;Theme4d&apos;, $User.UIThemeDisplayed == &apos;Theme4t&apos;, $User.UIThemeDisplayed == &apos;Theme4u&apos;),
HYPERLINK(&quot;/apex/Apttus__LightningSelectTemplate?id=&quot; &amp;Id &amp; &quot;&amp;action=Generate_Supporting_Document&amp;templateType=Supporting_Document&quot;,
IMAGE(&quot;/resource/Apttus__LightningResource/images/actions/generatesupporting.png&quot;, &quot;Generate Supporting Document&quot;),&quot;_self&quot;),
HYPERLINK(&quot;/apex/Apttus__SelectTemplate?id=&quot; &amp;Id &amp; &quot;&amp;action=Generate_Supporting_Document&amp;templateType=Supporting_Document&quot;,
IMAGE(&quot;/resource/Apttus__LightningResource/images/actions/generatesupporting.png&quot;,&quot;Generate Supporting Document&quot;),&quot;_self&quot;)), NULL)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Generate Contract Summary</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
