    <validationRules>
        <fullName>GDMFSL_Integrated_Location_Not_Editable</fullName>
        <active>true</active>
        <description>6/4/21 9464 - Data specialist needs edit access to these fields at all times. Sharing settings has this object as read only for all other personas</description>
        <errorConditionFormula>AND((NOT($Profile.Name=&apos;Data Load API Only&apos;)&amp;&amp; NOT($Profile.Name=&apos;System Admin API Only&apos;)),(NOT(CONTAINS($User.Full_Name__c,&apos;Interface User&apos;))&amp;&amp; NOT(CONTAINS($User.Full_Name__c,&apos;HBT API Integration&apos;))),(CONTAINS(CreatedBy.Full_Name__c,&apos;Interface User&apos;) || CONTAINS(CreatedBy.Full_Name__c,&apos;HBT API Integration&apos;)), ISCHANGED(Name))</errorConditionFormula>
        <errorMessage>Cannot update a Record which is created from Integration</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GDMFSL_Positive_Default_Dead_Man_Switch</fullName>
        <active>true</active>
        <errorConditionFormula>GDMFSL_Default_Dead_Man_Switch_Timer__c &lt; 0</errorConditionFormula>
        <errorDisplayField>GDMFSL_Default_Dead_Man_Switch_Timer__c</errorDisplayField>
        <errorMessage>The Default Dead Man Switch Value cannot be negative.</errorMessage>
    </validationRules>
