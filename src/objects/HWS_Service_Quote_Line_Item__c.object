    <validationRules>
        <fullName>HWS_Discount_Can_Not_Be_Negative</fullName>
        <active>true</active>
        <errorConditionFormula>HWS_Discount__c &lt;0</errorConditionFormula>
        <errorDisplayField>HWS_Discount__c</errorDisplayField>
        <errorMessage>Discount can not be less than zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HWS_Fee_Inclusion_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>Fee_Inclusion_flag__c</errorConditionFormula>
        <errorMessage>quote must contain at least one exclusion condition</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HWS_List_Price_Can_Not_Be_Negative</fullName>
        <active>true</active>
        <errorConditionFormula>HWS_Unit_List_Price__c &lt; 0</errorConditionFormula>
        <errorDisplayField>HWS_Unit_List_Price__c</errorDisplayField>
        <errorMessage>Unit List Price can not be less than zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HWS_Prevent_FeeInclusionQuantityEdit</fullName>
        <active>true</active>
        <description>Quantity and Fee Inclusion should not be allowed to Edit if the Quote is Accepted by Customer or Accepted by SME or Rejected by SME or Cancelled or Completed</description>
        <errorConditionFormula>AND(
OR(ISPICKVAL( Service_Quote__r.HWS_Quote_Status__c,&apos;Accepted by Customer&apos;),
ISPICKVAL( Service_Quote__r.HWS_Quote_Status__c,&apos;Accepted by CDM&apos;) ,
ISPICKVAL( Service_Quote__r.HWS_Quote_Status__c,&apos;Rejected by CDM&apos;) ,
ISPICKVAL( Service_Quote__r.HWS_Quote_Status__c,&apos;Cancelled&apos;) ,
ISPICKVAL( Service_Quote__r.HWS_Quote_Status__c,&apos;Completed&apos;),
ISPICKVAL( Service_Quote__r.HWS_Quote_Status__c,&apos;OC Review&apos;)),
OR(ISCHANGED(HWS_Quantity__c), ISCHANGED(HWS_Fee_Inclusion__c)))</errorConditionFormula>
        <errorMessage>Quantity and Fee Inclusion fields cannot be updated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HWS_Quantity_Can_Not_Be_Negative</fullName>
        <active>true</active>
        <errorConditionFormula>OR(HWS_Quantity__c   &lt;= 0 ,
			ISBLANK( HWS_Quantity__c ))</errorConditionFormula>
        <errorDisplayField>HWS_Quantity__c</errorDisplayField>
        <errorMessage>Quantity can not be less than zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HWS_Update_Exclusion_Category</fullName>
        <active>true</active>
        <description>HWSDDP-87</description>
        <errorConditionFormula>AND(ISCHANGED(HWS_Exclusion_Category__c) ,OR(NOT(ISPICKVAL(Service_Quote__r.HWS_Quote_Status__c, &apos;On Hold&apos;)) ,NOT(ISPICKVAL(Service_Quote__r.HWS_Quote_Status_Reason__c,&apos;Category Error&apos;))))</errorConditionFormula>
        <errorDisplayField>HWS_Exclusion_Category__c</errorDisplayField>
        <errorMessage>Exclusion Category field cannot be updated</errorMessage>
    </validationRules>
