    <validationRules>
        <fullName>Lock_Task_when_Completed</fullName>
        <active>true</active>
        <description>This validation rule will not allow task updation once it is marked completed.</description>
        <errorConditionFormula>isTaskCompleted__c  = true</errorConditionFormula>
        <errorMessage>This task is completed and cannot be updated anymore.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NF_Not_Allow_To_Update_Task</fullName>
        <active>true</active>
        <description>IWA #Req4419
Sprint 16</description>
        <errorConditionFormula>AND(ISPICKVAL(Status ,&apos;Closed - Not Completed&apos;), OR( ISCHANGED(Subject),ISCHANGED(ActivityDate), ISCHANGED(WhoId), ISCHANGED(Description), ISCHANGED(OwnerId), ISCHANGED(Priority),ISCHANGED(WhatId)) ,$Profile.Name&lt;&gt;$Label.Data_Loader_Profile_Name)</errorConditionFormula>
        <errorMessage>The creation of new tasks in closed opportunities is not allowed.</errorMessage>
    </validationRules>
