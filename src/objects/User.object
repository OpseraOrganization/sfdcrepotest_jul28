<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>Manager_of_NSA_partner_must_be_NSA</fullName>
        <active>true</active>
        <description>1220</description>
        <errorConditionFormula>AND ( 
Manager.NSA_Compliant__c = FALSE,ISPICKVAL(Contact.Account.Customer_Compliance__c,&apos;US_Govern&apos;), 
OR(Profile.Name = &apos;PRM Partner Community User Login&apos;,Profile.Name = &apos;PRM Partner Relationship Manager Login&apos;) 
)</errorConditionFormula>
        <errorMessage>Manager of a NSA  Partner user must be NSA</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_DM_user_able_to_create_Partner_user</fullName>
        <active>true</active>
        <description>Restrict non Data Migration users from creation partners users.  Under condition if contact is already in progress of onboarding.</description>
        <errorConditionFormula>AND(NOT(OR($Profile.Name = &apos;Integration API Only Profile&apos;,
$Profile.Name = &apos;Data Loader Profile&apos;)),
NOT(ISBLANK(ContactId)),
NOT(ISPICKVAL(Contact.Authentication_Status__c, &apos;&apos;)),
ISNEW()
)</errorConditionFormula>
        <errorMessage>You are not authorized to create Partner Users</errorMessage>
    </validationRules>
</CustomObject>
