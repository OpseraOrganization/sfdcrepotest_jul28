<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>NSA_User_if_Role_is_NSA</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( 
NSA_Compliant__c = false,
$User.FirstName&lt;&gt;&apos;Informatica&apos;,
OR( 
UserRole.Name = &apos;Global&apos;, 
UserRole.Name = &apos;NSA&apos;, 
UserRole.Name = &apos;North America NSA - Regional Sales Lead/Direct Sales Manager&apos;
))</errorConditionFormula>
        <errorMessage>This role cannot be assigned to a non NSA compliant user.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Nokia_Id_Number_only</fullName>
        <active>false</active>
        <description>Nokia Id field can accept number ony</description>
        <errorConditionFormula>IF( (Profile.Name==&apos;Customers&apos; || Profile.Name==&apos;Customers Login&apos; || Profile.Name==&apos;PRM Partner Community User Login&apos; ||Profile.Name == &apos;PRM Partner Relationship Manager Login&apos; ||Profile.Name== &apos;Marketo Sync&apos; || Profile.Name== &apos;Integration API Only Profile&apos; || Profile.Name==&apos;Excel Add-In Profile&apos; ||Profile.Name== &apos;Data Loader Profile&apos;) , false, ISNUMBER( Nokia_ID__c ) = FALSE)</errorConditionFormula>
        <errorDisplayField>Nokia_ID__c</errorDisplayField>
        <errorMessage>The field accepts number only.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Not_editable_user_name_fields</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(ISCHANGED(FirstName ),
  ISCHANGED( LastName ),
  ISCHANGED(MiddleName),
  ISCHANGED(Email),
  ISCHANGED(Suffix),
  ISCHANGED(Country__c)),
OR($Profile.Name = &apos;PRM Partner Community User Login&apos;,
$Profile.Name = &apos;PRM Partner Relationship Manager Login&apos;, $Profile.Name = &apos;PRM Unauthorized Partner User Login&apos;
)
)</errorConditionFormula>
        <errorMessage>You are not authorized to modify these fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_EMAIL_change_on_User_record</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
				ISCHANGED(Email), 
				$Profile.Name&lt;&gt;&apos;System Administrator&apos;,
				$Profile.Name&lt;&gt;&apos;Global Process Admin Profile&apos;,
				$User.FirstName&lt;&gt;&apos;Informatica&apos;,
				$User.FirstName&lt;&gt;&apos;iPAAS&apos;
)</errorConditionFormula>
        <errorMessage>You are not authorized to change email address.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Users_Deactivation_Reason</fullName>
        <active>true</active>
        <description>to ensure that &quot;Do Not Deactivate User&quot; reason is captured whenever this checkbox is checked</description>
        <errorConditionFormula>AND( 
Do_Not_Deactivate_User__c = TRUE, 
ISBLANK(TEXT( Do_not_deactivate_reason__c )) 
)</errorConditionFormula>
        <errorDisplayField>Do_not_deactivate_reason__c</errorDisplayField>
        <errorMessage>Please pick the reason why this user should not be deactivated</errorMessage>
    </validationRules>
