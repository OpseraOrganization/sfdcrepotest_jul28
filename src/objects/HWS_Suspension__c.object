    <validationRules>
        <fullName>CH_Validate_Suspension_Status</fullName>
        <active>true</active>
        <description>https://jiradc2.ext.net.nokia.com/browse/HWSDDP-75#</description>
        <errorConditionFormula>IF (

	!ISNEW(),
	
	IF(  ISCHANGED(HWS_Suspension_Status__c),
	
		IF(
			AND(
				TEXT(PRIORVALUE( HWS_Suspension_Status__c))=&apos;Pending Review&apos; ,
				AND(TEXT(HWS_Suspension_Status__c) != &apos;Suspended&apos;,
				   TEXT(HWS_Suspension_Status__c)!=&apos;Rejected&apos;													
				)
			),
			TRUE,
			IF(
				AND(
					TEXT(PRIORVALUE( HWS_Suspension_Status__c))=&apos;Suspended&apos; ,
					TEXT(HWS_Suspension_Status__c) != &apos;UnSuspended&apos;
				),
				TRUE,
				IF(
					AND(
						TEXT(PRIORVALUE( HWS_Suspension_Status__c))=&apos;UnSuspended&apos; ,
						TEXT(HWS_Suspension_Status__c) != &apos;Pending Review&apos;
					),
					TRUE,
						IF(
							AND(
								TEXT(PRIORVALUE( HWS_Suspension_Status__c))=&apos;Rejected&apos; ,
								TEXT(HWS_Suspension_Status__c) != &apos;Pending Review&apos;
							),
							TRUE,
							FALSE
			
						)
		
				)
		
			)
		
		)
	
	,
	
	FALSE
	
	
	)
	
	,
	
	FALSE

)</errorConditionFormula>
        <errorDisplayField>HWS_Suspension_Status__c</errorDisplayField>
        <errorMessage>Invalid Status Transition</errorMessage>
    </validationRules>
