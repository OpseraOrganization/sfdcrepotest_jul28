  <validationRules>
    <fullName>Valid_Level_Reseller_GPP_2_0_Maintenance</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
	$Profile.Name != 'Data Loader Profile',
	RecordType.Name == 'Reseller GPP 2.0',
	Text(Accreditation_Type__c) == 'Maintenance',
	OR(
	NOT(OR(
		Text(Accreditation_Level__c) = 'No Accreditation',
		Text(Accreditation_Level__c) = 'Basic',
		Text(Accreditation_Level__c) = 'Premium',
		Text(Accreditation_Level__c) = 'Professional'
		) ),
	NOT(OR(
