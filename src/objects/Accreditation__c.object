    <validationRules>
        <fullName>AccCurrencyShouldEqualPriceListCurrency</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT( ISBLANK(NokiaCPQ_Contract_Price_List__c) ) , NokiaCPQ_Account_Currency__c &lt;&gt; TEXT(NokiaCPQ_Contract_Price_List__r.CurrencyIsoCode))</errorConditionFormula>
        <errorDisplayField>NokiaCPQ_Contract_Price_List__c</errorDisplayField>
        <errorMessage>No Contract PriceList Exists for this Account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Incoterm_Error_Maintenance</fullName>
        <active>true</active>
        <description>For Demo Purpose.</description>
        <errorConditionFormula>And(
OR(
RecordType.DeveloperName = &apos;Distributor_GPP_3_0&apos;,
RecordType.DeveloperName = &apos;Reseller_GPP_3_0&apos;
),
ISPICKVAL(Accreditation_Type__c , &apos;Maintenance&apos;),


OR(
NOT( ISBLANK( NokiaCPQ_Incoterm_Percentage__c) ),
NOT( ISBLANK( Text(NokiaCPQ_Incoterm__c)) )
)
  
)</errorConditionFormula>
        <errorMessage>Incoterm can only be filled for Product Type only</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Incoterm_Not_Greated_Than_20</fullName>
        <active>true</active>
        <errorConditionFormula>NokiaCPQ_Incoterm_Percentage__c  &gt; 0.50</errorConditionFormula>
        <errorMessage>Incoterm % should not be greater than 50</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRM_Display_on_Partner_Locator</fullName>
        <active>true</active>
        <description>SFPRM-490</description>
        <errorConditionFormula>AND(
    Partner_Type__c = &apos;Indirect Reseller&apos;,
    ISPICKVAL(PRM_Display_on_Partner_Locator__c, &apos;Yes&apos;)  
)</errorConditionFormula>
        <errorDisplayField>PRM_Display_on_Partner_Locator__c</errorDisplayField>
        <errorMessage>Indirect Reseller Accounts can&apos;t be displayed on the Partner Locator.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PRM_Display_on_Partner_Locator_Aligned</fullName>
        <active>true</active>
        <description>SFPRM-492</description>
        <errorConditionFormula>AND(
				ISPICKVAL(PRM_Display_on_Partner_Locator__c, &apos;Yes&apos;),
				NOT(ISPICKVAL(Partner__r.Co_op_Account_Created__c, &apos;Yes&apos;))
)</errorConditionFormula>
        <errorDisplayField>PRM_Display_on_Partner_Locator__c</errorDisplayField>
        <errorMessage>You can only select &apos;Yes&apos; if Display on Partner Locator on the Account is set to &apos;Yes&apos;.</errorMessage>
    </validationRules>
