    <validationRules>
        <fullName>HWS_ValidateFee</fullName>
        <active>true</active>
        <description>HWSDDP-24</description>
        <errorConditionFormula>OR(
INCLUDES (HWS_SupportedQuoteTypes__c, &quot;Damaged Part&quot;) &amp;&amp; isblank (HWS_DamagedHWRepairFee__c),
INCLUDES (HWS_SupportedQuoteTypes__c, &quot;Damaged Part&quot;) &amp;&amp; isblank (HWS_DamagedHWReplacementFee__c),
INCLUDES (HWS_SupportedQuoteTypes__c, &quot;Damaged Part&quot;) &amp;&amp; isblank (HWS_RepackingHWPackingFee__c),
INCLUDES (HWS_SupportedQuoteTypes__c, &quot;Damaged Part&quot;) &amp;&amp; isblank (HWS_HWAssessmentFee__c),
INCLUDES (HWS_SupportedQuoteTypes__c, &quot;Damaged Part&quot;) &amp;&amp; isblank (HWS_DocumentationFee__c),

INCLUDES(HWS_SupportedQuoteTypes__c, &quot;Unreturned Part&quot;) &amp;&amp; isblank (HWS_HWUnreturnedPartFee__c)
)</errorConditionFormula>
        <errorMessage>For Damaged Part quote type, do not populate HW Unreturned Part Fee.

For Unreturned Part quote type, do not populate fields related to Damaged HW fees.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HWS_ValidateProduct</fullName>
        <active>true</active>
        <description>HWSDDP-24</description>
        <errorConditionFormula>AND(RecordType.DeveloperName = &apos;HWS_Part_Service_Fee&apos;,
ISBLANK(HWS_Product__c)
)</errorConditionFormula>
        <errorMessage>Product cannot be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HWS_ValidateReadonly</fullName>
        <active>true</active>
        <description>HWSDDP-24</description>
        <errorConditionFormula>AND( 
    NOT(ISBLANK(HWS_Product__c)),
    NOT(ISBLANK(HWS_OrderCode__c)),
        OR( 
        ISCHANGED(HWS_OrderCode__c), 
        ISCHANGED(HWS_Product__c ), 
        ISCHANGED(CurrencyIsoCode)
	
       ) 
)</errorConditionFormula>
        <errorMessage>The Product, Order Code and Currency cannot be changed</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
